---
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../../consts';
import { getLangFromUrl, useTranslations } from '../../i18n/utils';
let langCode = Astro.currentLocale;
console.log('Current locale:', Astro.currentLocale);
if(langCode == undefined || !["en", "es", "fr", "de"].includes(langCode)) {
	langCode = 'en';
}
const lang = getLangFromUrl(Astro.url) || langCode;
Astro.redirect('/'+lang);
const t = useTranslations(lang);
const item =  {
	"@context": "https://schema.org",
	"@type": "Organization",
	"name": "Taxi 21 Cala Rajada",
	"url": "https://taxicalaratjada.es/",
	"logo": "https://taxicalaratjada.es/img/logo_negro_tranparente.png",
	"description":t('seo.description'),
	"contactPoint": [
		{
			"@type": "ContactPoint",
			"contactType": "Customer Service",
			"url": "https://wa.me/34645329809",
			"name": "WhatsApp"
		},
		{
			"@type": "ContactPoint",
			"contactType": "Customer Support",
			"telephone": "+34 645 32 98 09",
			"name": "Phone"
		}
	]
}
